Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FifoSchedulling", "[", 
    RowBox[{"arrivalsTime_", ",", "serviceTime_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "checkTime"}], "}"}], ",", 
     RowBox[{
      RowBox[{"n", "=", "1"}], ";", 
      RowBox[{"checkTime", "=", 
       RowBox[{"arrivalsTime", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"checkTime", "\[GreaterEqual]", "#"}], ",", 
            RowBox[{"checkTime", "+=", 
             RowBox[{"serviceTime", "[", 
              RowBox[{"[", 
               RowBox[{"n", "++"}], "]"}], "]"}]}], ",", 
            RowBox[{"checkTime", "=", 
             RowBox[{"#", "+", 
              RowBox[{"serviceTime", "[", 
               RowBox[{"[", 
                RowBox[{"n", "++"}], "]"}], "]"}]}]}]}], "]"}], ")"}], "&"}], 
        ",", "arrivalsTime"}], "]"}]}]}], "]"}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CalculateMM1Packets", "[", 
    RowBox[{"insertionTime_", ",", " ", "serviceTime_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "n", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Map", "[", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"{", 
           RowBox[{"#", ",", " ", 
            RowBox[{"9600", "*", 
             RowBox[{"serviceTime", "[", 
              RowBox[{"[", 
               RowBox[{"n", "+", "1"}], "]"}], "]"}]}], ",", 
            RowBox[{"n", "++"}], ",", " ", "0", ",", " ", "0"}], "}"}], ")"}],
          "&"}], ",", " ", "insertionTime"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SimTxMM1", "[", 
   RowBox[{
   "lamdaMM1_", ",", "muMM1_", ",", "nPacketsMM1_", ",", "tpropMM1_", ",", 
    "tsMM1_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "arrivalsTimeMM1", ",", " ", "serviceTimeMM1", ",", "departureTimeMM1", 
      ",", "insertionTimeMM1", ",", "  ", "packetsMM1"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"arrivalsTimeMM1", "=", 
       RowBox[{"Accumulate", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"RandomExp", "[", "lamdaMM1", "]"}], ",", "nPacketsMM1"}], 
         "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"serviceTimeMM1", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"RandomExp", "[", "muMM1", "]"}], ",", " ", "nPacketsMM1"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"SetIniParDraw", "[", 
       RowBox[{"tpropMM1", ",", "tsMM1"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"departureTimeMM1", "=", 
       RowBox[{"FifoSchedulling", "[", 
        RowBox[{"arrivalsTimeMM1", ",", " ", "serviceTimeMM1"}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"insertionTimeMM1", "=", 
       RowBox[{"departureTimeMM1", "-", "serviceTimeMM1"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"packetsMM1", "=", 
       RowBox[{"CalculateMM1Packets", "[", 
        RowBox[{"insertionTimeMM1", ",", " ", "serviceTimeMM1"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
